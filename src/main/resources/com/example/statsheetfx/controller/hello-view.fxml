<AnchorPane prefHeight="349.0" prefWidth="581.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.statsheetfx.controller.HelloController">
   <children>
      <Label fx:id="welcome" layoutX="207.0" layoutY="23.0" text="StatsheetFX">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" />
      <Button fx:id="newGame" layoutX="222.0" layoutY="100.0" mnemonicParsing="false" onAction="#newGameClicked" text="New Game">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="btn_createTeamPlayer" layoutX="176.0" layoutY="176.0" mnemonicParsing="false" onAction="#createTeamsPlayersClicked" text="Create Team/Players">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="btn_analysisWindow" layoutX="237.0" layoutY="252.0" mnemonicParsing="false" onAction="#analysisWindowOpen" text="Analysis">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
